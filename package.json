    "@types/node": "13.9.1",
    "@types/request-promise": "4.1.47",
    "@types/sanitize-html": "1.27.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.1.0",
    "auto-changelog": "2.3.0",
    "jest": "27.2.5",
    "lint-staged": "10.4.1",
    "micromatch": "4.0.4",
    "prettier": "1.19.1",
    "supertest": "^4.0.2",
