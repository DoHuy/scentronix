    "@nestjs/swagger": "5.1.0",
    "@nestjs/terminus": "10.2.3",
    "@nestjs/throttler": "2.0.0",
    "@nestjs/typeorm": "8.0.2",
    "axios": "0.21.4",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "dotenv": "10.0.0",
    "express-timeout-handler": "^2.2.2",
    "faker": "5.5.3",
    "hash-stream": "1.2.0",
